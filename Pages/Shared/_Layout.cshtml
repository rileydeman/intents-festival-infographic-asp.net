<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Intents Festival</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/intents_festival_infographic.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <p>Header</p>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            <p>&copy; 2023-@DateTime.Now.Year rileydeman</p>
        </div>
    </footer>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>